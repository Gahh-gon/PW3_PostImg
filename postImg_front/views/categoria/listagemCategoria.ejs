<%- include('../partials/header.ejs')  %>

    <title>LISTAGEM DE IMAGENS</title>

</head>

<body>
    
    <div class="container">

        <%- include('../partials/menu.ejs')  %>

        <div class="card mt-2">
            <div class="card-header  text-center">LISTAGEM DE IMAGENS</div>
        </div>

        <%
            categoria.forEach(categoria=>{
        %>

            <div class="card mt-2">

                <div class="card-body">
                <%=categoria.nomeCategoria %>
                    
                </div>

                <a href="/formEdicaoCategorias/<%= categoria.cod_categoria %>" class="btn btn-info mt-1">EDITAR</a>
                <a href="/excluirCategoria/<%= categoria.cod_categoria %>" class="btn btn-danger mt-1">EXCLUIR</a>

            </div>

        <% }) %>

    </div>

</body>

<%- include('../partials/footer.ejs')  %>